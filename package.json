{
  "name": "execbg",
  "displayName": "ExecBG",
  "description": "Execute commands from VS Code in the background",
  "version": "0.1.0",
  "publisher": "daaku",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daaku/vscode-execbg/issues"
  },
  "homepage": "https://github.com/daaku/vscode-execbg",
  "repository": {
    "type": "git",
    "url": "https://github.com/daaku/vscode-execbg"
  },
  "galleryBanner": {
    "theme": "light"
  },
  "activationEvents": [
    "onCommand:execbg.run"
  ],
  "main": "./out/index.js",
  "contributes": {
    "commands": [
      {
        "command": "execbg.run",
        "title": "Run Command in Background",
        "category": "ExecBG"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "tslint -p ./"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@types/vscode": "^1.44.0",
    "typescript": "^3.8.3",
    "vsce": "^1.75.0"
  },
  "dependencies": {
    "tree-kill": "^1.2.2"
  }
}
