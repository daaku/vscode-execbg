{
  "name": "execbg",
  "displayName": "ExecBG",
  "description": "Execute commands from VS Code in the background",
  "version": "0.4.0",
  "publisher": "daaku",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daaku/vscode-execbg/issues"
  },
  "homepage": "https://github.com/daaku/vscode-execbg",
  "repository": {
    "type": "git",
    "url": "https://github.com/daaku/vscode-execbg"
  },
  "galleryBanner": {
    "theme": "light"
  },
  "activationEvents": [
    "onCommand:execbg.run"
  ],
  "main": "./dist/index.js",
  "contributes": {
    "commands": [
      {
        "command": "execbg.run",
        "title": "Run Command in Background",
        "category": "ExecBG"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "dev": "webpack --mode development --watch"
  },
  "devDependencies": {
    "@types/node": "^15.0.2",
    "@types/vscode": "^1.56.0",
    "@types/webpack": "^4.41.22",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.4",
    "vsce": "^1.88.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "tree-kill": "^1.2.2"
  }
}
